<div id='actions'>
  <h2><%= t('.details')%></h2>
  <p>
    <strong><%= t('.age') %>:</strong> <%= time_ago_in_words(@registration.created_at) %>
  </p>
  <h2><%= t('.actions') %></h2>
  <ul>
    <li>
    <%= link_to t('.back_to_all_registrations'), refinery.registrations_admin_registrations_path,
                   :class => "back_icon" %>
    </li>
    <li>
      <%= link_to t('delete_registration', :scope => 'refinery.registrations.admin.registrations.registration'),
                  refinery.registrations_admin_registration_path(@registration),
                  :class => 'delete_icon no-tooltip confirm-delete',
                  :title => t('delete_registration', :scope => 'refinery.registrations.admin.registrations.registration'),
                  :'data-confirm' => t('message', :scope => 'refinery.admin.delete', :title => @registration.name),
                  :'data-method' => "delete" %>
    </li>
  </ul>
</div>
